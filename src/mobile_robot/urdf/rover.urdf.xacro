    <visual name="">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <cylinder radius="0.15" length="0.1"/>
        </geometry>
        <material name="black">
            <color rgba="0.0 0.0 0.0 1.0"/>
        </material>
    </visual>
    <collision>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <geometry>
            <cylinder radius="0.15" length="0.1"/>
        </geometry>
    </collision>
</link>

<joint name="wheel_${rsider}_joint" type="continuous">
    <origin xyz="0.250 0.40 0.0" rpy="1.57 0.0 0.0"/>
    <parent link="base_link"/>
    <child link="wheel_${rsider}_link"/>
    <axis xyz="0.0 0.0 1.0"/>
</joint>

